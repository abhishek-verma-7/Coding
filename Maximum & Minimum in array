int maximumElement(int arr[],int n)
{
    //Your code here
    int max = INT_MIN;
    for(int i = 0; i < n; i++)
    {
        if(arr[i] > max)
            max = arr[i];
    }
    return max;
}

int minimumElement(int arr[],int n)
{
    //Your code here
    int min = INT_MAX;
    for(int i = 0; i < n; i++)
    {
        if(arr[i] < min)
            min = arr[i];
    }
    return min;
}
