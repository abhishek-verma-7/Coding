void square(int n){
    // code here
     int count=n;
    for(int i=0;i<n;i++){
        if(i==0){
            count=n;
            while(count-->0)cout<<"* ";
        }
        else if(i==n-1){
            count=n;
            while(count-->0)cout<<"* ";
        }
        else{
            count=2*n-3;
            cout<<"*";
            while(count-->0)cout<<" ";
            cout<<"*";
        }
        cout<<endl;
    }
}
